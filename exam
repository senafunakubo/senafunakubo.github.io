#import <Cocoa/Cocoa.h>
#import "Products.h"
#import "Drinks.h"
#import "Food.h"
#import "Cloth.h"
#import "Material.h"
#import "ShoppingCart.h"

int main(int argc, const char * argv[]) {
    return NSApplicationMain(argc, argv);
    NSMutableArray<Products*>* allKindOfProducts = [[NSMutableArray alloc] init];

    Drinks* drink1 = [[Drinks alloc]initWithProductId:412 productName:@"Pepsi" productPrice:2 productMadeInCountry:@"USA" drinkSize:150];
    Drinks* drink2 = [[Drinks alloc]initWithProductId:183 productName:@"Ginger Zero" productPrice:3 productMadeInCountry:@"Canada" drinkSize:200];
    Food* food1 = [[Food alloc]initWithProductId:100 productName:@"Chicken" productPrice:8 productMadeInCountry:@"Canada" foodCalorie:350 foodSize:4 foodIngredients:];

    [allKindOfProducts addObject:drink1];
    [allKindOfProducts addObject:drink2];


}
